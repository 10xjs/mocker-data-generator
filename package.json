{
  "name": "mocker-data-generator",
  "version": "1.0.2",
  "description": "A simplified way to generate mock data, builds using a simple schema and with the FakerJs",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/danibram/mocker-data-generator"
  },
  "keywords": [
    "mock",
    "faker",
    "fakerjs",
    "json",
    "fake",
    "mocks"
  ],
  "author": {
    "name": "Daniel Biedma Ramos",
    "email": "info@dbr.io",
    "url": "dbr.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danibram/mocker-data-generator/issues"
  },
  "scripts": {
    "build": "node_modules/.bin/babel -s -d lib src",
    "dev": "npm run build -- -w",
    "test": "./node_modules/.bin/mocha ./test/mocker.test.js",
    "test:newWay": "mocha -R spec --require source-map-support/register lib/__test__/*.spec.js ",
    "test:dev": "npm run test -- -w"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "chance": "^0.8.0",
    "faker": "^3.0.1"
  }
}
