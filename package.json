    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.2",
    "mocha": "^3.1.2",
    "source-map-support": "^0.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "casual": "^1.5.6",
    "chance": "^1.0.0",
    "faker": "^3.0.1",
    "randexp": "^0.4.2"
  }
}
